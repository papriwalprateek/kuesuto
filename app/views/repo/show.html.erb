<div class=" content_window">
<ul class="breadcrumbs add_bar"> 
	<li><a href="/">repositories</a></li> 
	<li class="current"><a href="#"><%=@repo%></a></li> 
</ul>
<div class="row explorer" data-equalizer>
<div class="large-3 medium-5 columns side-nav explorer_column" data-equalizer-watch style="padding:0px;">

<ul class="side-nav explorer_column_s">
	<li><a href="/repo/algorithm/seq" data-remote="true"><span class="nav_icon1 nav_folder"></span>Sequence Sorting</a></li> 	
	<li><a href="/repo/algorithm/graph" data-remote="true"><span class="nav_icon1 nav_folder"></span>Graph Algorithms</a></li> 	
	
</ul>
</div>
<div class="large-3 medium-5 columns" data-equalizer-watch>
<div class="box" id="items_grid">
</div>
</div>
<div class="large-5 medium-5 columns end" data-equalizer-watch>
<%=render 'layouts/temp'%>
</div>

</div>
</div>	
<style>
.explorer_column_s{
	padding: 0px;
}
.explorer_column_s li a{
	line-height: 26px;
}
.nav_icon1{
 width:20px; 
   height:20px;
   margin: 3px;
   margin-right: 6px;
   display: block;
   float: left;
	
}
.nav_folder{
	
  	background: url(<%= asset_path 'folder.png' %>) left top;

}
.nav_entity{
	
 	background: url(<%= asset_path 'entity.png' %>) left top;

}
.explorer_column_s li a:hover:after {
    content: url(<%= asset_path 'arrow_right.png' %>);
    float: right;
    display: block;
   width:20px; 
   height:20px;
   margin-top: 2px;
}
.explorer_column{
	border-right: 1px solid #dedede;
