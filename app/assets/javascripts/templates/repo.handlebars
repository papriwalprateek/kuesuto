<div class="small-12 medium-12 large-12 columns" style="margin-bottom:1.5rem">
	
<ul class="breadcrumbs add_bar"> 


{{#each controller.breadcrumbs}}
	<li {{bind-attr class=active}}>{{#link-to 'repo' url}}{{name}}{{/link-to}}</li> 
{{/each }}

</ul>
</div>
<div class="small-12 columns side-nav"  style="padding:0px;">
{{#ifeq controller.has "list"}}
	{{partial "partials/entitylist"}}
{{/ifeq}}


{{#ifeq controller.has "tiles"}}
	{{partial "partials/entitypage"}}
{{/ifeq}}

{{#ifeq controller.has "error"}}
	{{{controller.error}}}
{{/ifeq}}

{{#ifeq controller.has "repos"}}
	{{#each controller.repos}}
		{{#view App.ExploreButtonView}}	
		{{name}}
		{{/view}}
	{{/each}}
{{/ifeq}}

</div>

 